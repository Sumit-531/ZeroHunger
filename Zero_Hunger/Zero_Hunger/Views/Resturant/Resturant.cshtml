@model IEnumerable<ZeroHunger.Ef.Models.CollectRequests>

@{
    ViewBag.Title = "Resturant";
}

<h2>Resturant</h2>

<p>
    @Html.ActionLink("Add New Request", "UpdateReq")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FoodType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoodType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CrId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CrId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CrId })
        </td>
    </tr>
}

</table>
